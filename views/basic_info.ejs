                            <div class="jumbotron">
                                <h1 class="display-4">Opps!!!</h1>
                                <p class="lead">You don't have any Numerical data</p>
                            </div>
                            <% } else{%>
                            <div class="wrap-table100">
                                <div class="table100 border">
                                    <table>
                                        <thead>
                                            <tr class="table100-head">
                                                <th>columns</th>
                                                <% list.numerical.forEach(function(col) { %>
                                                <th><%= col %></th>
                                                <% });  %>
                                            </tr>
                                        </thead>
